# 3.custom bindings
#escape Super_L
bind space exec ~/.config/rofi/launchers/misc/launcher.sh
bind q quit
bind Tab next
bind ISO_Left_Tab prev
bind p exec poweroff
definekey top M-Tab next
definekey top M-w kill
definekey top Super_L exec ~/.config/rofi/launchers/misc/launcher.sh

# startup apps
exec nitrogen --restore
exec urxvt

# unbind unused bindings
